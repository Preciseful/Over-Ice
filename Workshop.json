{
  "events": {
    "localized": "Ongoing - Each Player",
    "globalized": "Ongoing - Global"
  },

  "actions": {
    "wait": {
      "args": {
        "duration": {
          "type": "float",
          "index": 0,
          "default": "0.016"
        },
        
        "behavior": {
          "type": "WaitBehav",
          "index": 1,
          "default": "Ignore Condition"
        }
      },
      
      "translation": "Wait",
      "type": "void"
    },
    
    "append": {
      "type": "void",
      "lhv-only": "define"
    },

    "remove": {
      "type": "void",
      "lhv-only": "define"
    },

    "removeAt": {
      "type": "void",
      "lhv-only": "define"
    },

    "createHud": {
      "args": {
        "visible": {
          "type": ["Player", "array Player"],
          "index": 0
        },

        "header": {
          "type": ["string", "float", "null"],
          "index": 1,
          "default": "Null"
        },

        "subheader": {
          "type": ["string", "floaat", "null"],
          "index": 2,
          "default": "Null"
        },

        "text": {
          "type": ["string", "floaat", "null"],
          "index": 3,
          "default": "Null"
        },

        "location": {
          "type": "HudLoc",
          "index": 4
        },

        "sort": {
          "type": "int",
          "index": 5,
          "default": "0"
        },

        "headerColor": {
          "type": "Color",
          "index": 6,
          "default": "Color(White)"
        },

        "subColor": {
          "type": "Color",
          "index": 7,
          "default": "Color(White)"
        },

        "textColor": {
          "type": "Color",
          "index": 8,
          "default": "Color(White)"
        },

        "reeval": {
          "type": "HudReeval",
          "index": 9,
          "default": "Visible To And String"
        },

        "spec": {
          "type": "SpecVisibil",
          "index": 10,
          "default": "Default Visibility"
        }
      },

      "translation": "Create Hud Text",
      "type": "void"
    },

    "destroyHud": {
      "args": {
        "hud": {
          "type": "Text",
          "index": 0
        }
      },

      "translation": "Destroy HUD Text",
      "type": "void"
    },

    "createIWT": {
      "args": {
        "visible": {
          "type": ["Player", "array Player"],
          "index": 0
        },

        "header": {
          "type": ["string", "float"],
          "index": 1
        },

        "position": {
          "type": ["Player", "Vector"],
          "index": 2
        },

        "scale": {
          "type": "float",
          "index": 3,
          "default": "1"
        },
        
        "clip": {
          "type": "Clip",
          "index": 4,
          "default": "Clip Against Surfaces"
        },

        "reeval": {
          "type": "IWTReeval",
          "index": 5,
          "default": "Visible To Position And String"
        },

        "color": {
          "type": "Color",
          "index": 6,
          "default": "Color(White)"
        },

        "spec": {
          "type": "SpecVisibil",
          "index": 7,
          "default": "Default Visibility"
        }
      },

      
      "translation": "Create In-World Text",
      "type": "void"
    },
    
    "createEffect": {
      "args": {
        "player": {
          "type": ["Player", "array Player"],
          "index": 0
        },
        
        "type": {
          "type": "Effect",
          "index": 1
        },

        "color": {
          "type": "Color",
          "index": 2
        },

        "position": {
          "type": ["Player", "Vector"],
